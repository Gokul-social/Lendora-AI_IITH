{
  "version": 2,
  "builds": [
    {
      "src": "frontend/Dashboard/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/Dashboard/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://your-backend-url.vercel.app/api/$1"
    },
    {
      "src": "/ws",
      "dest": "wss://your-backend-url.vercel.app/ws"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_WS_URL": "@vite_ws_url"
  }
}

